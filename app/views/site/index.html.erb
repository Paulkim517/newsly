      <li><a href="https://twitter.com/share/" class="twitter-share-button" data-url="http://newsli.herokuapp.com" data-via="sahash4" data-layout="button"><img src=<%= asset_path"twitter.png"%> alt="Share newsly" width="42" height="42" border="0"></a></li>
    </ul>
  </div>


    <div class="col-md-4" id="news">
      <% @both.each do |i| %>
      <div class="panel panel-default" id="news-feed">
        <div class="panel-heading">
          <h3 class="panel-title" id="tit"><%= i['title'] %></h3>
        </div>
        <div class="panel-body">
          <p id="p"><%= i['abstract'] %> <br><p id="linkp"><%= link_to 'View Article', i['url'] %></p></p>
        </div>
      </div><br>
      <% end %>

    </div>




